      <span>
        <svg focusable="false" viewBox="0 0 24 24" aria-hidden="true" width="100%" height="100%" version="1.1" xml:space="preserve" style="fill-rule: evenodd; clip-rule: evenodd; stroke-linecap: round; stroke-linejoin: round;"><g><path d="M22,1.481l-20,20" style="fill: none; fill-rule: nonzero; stroke-width: 4px;"></path><path d="M2.004,1.486l19.992,19.991" style="fill: none; fill-rule: nonzero; stroke-width: 4px;"></path></g>
        </svg>
      </span>
    </div>
    <div class="announcement_slider owl-carousel">
       {% for block in section.blocks %}
      <div class="announcement_slide"><p>{{ block.settings.info_title }}</p></div>
      {% endfor %}
    </div>

</div>